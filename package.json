		"express-session": "^1.15.6",
		"knex": "^0.15.2",
		"leaflet": "^1.3.4",
		"lodash": "^4.17.11",
		"lodash.debounce": "^4.0.8",
		"lusca": "^1.4.1",
		"mini-css-extract-plugin": "^0.4.3",
		"moment": "^2.22.2",
