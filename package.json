{
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"start": "run-p start:*",
		"start:dev-server": "nodemon",
		"start:webpack": "webpack --watch --mode development",
		"lint": "tslint -p ./tsconfig.json",
		"compile": "tsc -p ./tsconfig.json",
		"clean": "rm -rf ./build/dist",
		"copy-assets": "echo 'copying assets...' && cp .env build/dist/ && cp package.json build/dist/ && cp -R src/public build/dist/src && cp -R node_modules build/dist/node_modules",
		"verify": "yarn clean && yarn install --frozen-lockfile && yarn lint && yarn compile && yarn test",
		"build": "yarn verify && webpack --mode production && yarn install --frozen-lockfile --production && yarn copy-assets",
		"server-e2e-tests": "DATABASE_NAME=emcit_test DEBUG=knex:query,knex:bindings,knex:tx jest --runInBand --config=jest.server.e2e.config.json",
		"unit-tests": "jest --config=jest.server.unit.config.json --coverage --passWithNoTests && jest --config=jest.ui.unit.config.json --coverage --passWithNoTests",
		"test": "yarn server-e2e-tests",
		"testWatch": "jest --config=jest.config.json --watch",
		"create-migration": "knex migrate:make",
		"rollback-migration": "DEBUG=knex:query,knex:bindings,knex:tx knex migrate:rollback",
		"drop-database": "ts-node -r tsconfig-paths/register ./src/server/database/setup/dropDatabase"
	},
	"dependencies": {
		"@google-cloud/storage": "^2.4.2",
