	"scripts": {
		"start": "run-p start:*",
		"start:dev-server": "nodemon",
		"start:webpack": "webpack --watch --mode development",
		"lint": "tslint -p ./tsconfig.json",
		"build-ui": "webpack",
		"build": "rm -rf build/dist && yarn install && tsc && yarn install --production --ignore-scripts --prefer-offline && cp -R node_modules build/dist/node_modules && cp .env build/dist/ && cp package.json build/dist/",
		"server-e2e-tests": "DATABASE_NAME=emcit_test DEBUG=knex:query,knex:bindings,knex:tx jest --runInBand --config=jest.e2e.config.json",
		"unit-tests": "jest --config=jest.server.unit.config.json --coverage && jest --config=jest.ui.unit.config.json --coverage",
		"testWatch": "jest --config=jest.config.json --watch",
		"create-migration": "knex migrate:make",
		"create-database": "ts-node -r tsconfig-paths/register ./src/server/database/setup/createDatabase",
		"drop-database": "ts-node -r tsconfig-paths/register ./src/server/database/setup/dropDatabase"
	},
